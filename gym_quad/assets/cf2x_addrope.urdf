            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>

    <!-- Joint: Hook Joint -->
    <joint name="hook_joint" type="fixed">
        <parent link="center_of_mass_link" />
        <child link="hook" />
        <origin rpy="0 0 0" xyz="0 0 -0.03" />
    </joint>

    

